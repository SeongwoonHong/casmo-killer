{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "jsx-a11y",
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-undef": "off",
    "function-paren-newline": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "comma-dangle": 0,
    "global-require": 0,
    "import/no-extraneous-dependencies": 0,
    "no-param-reassign": 0
  }
}
